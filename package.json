{
  "name": "eggbasicproject",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-register": "^6.26.0",
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-mysql": "^3.0.0",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^5.2.1",
    "egg-swagger-doc-feat": "^2.2.13",
    "egg-validate": "^2.0.2",
    "egg-view-ejs": "^2.0.0",
    "js-md5": "^0.7.3",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "sequelize-cli": "^5.5.1",
    "webpack": "^4.41.6"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=webgl-backend",
    "stop": "egg-scripts stop --title=webgl-backend",
    "dev": "egg-bin dev env:sit",
    "dev:sit": "egg-bin dev --env=sit",
    "dev:pre": "egg-bin dev --env=pre",
    "dev:prd": "egg-bin dev --env=prd",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "m:new": "sequelize migration:create --name",
    "m:up": "sequelize db:migrate",
    "m:down": "sequelize db:migrate:undo"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "plugins": [
    "transform-decorators"
  ],
  "author": "caiyong",
  "license": "MIT"
}
